local MainSection = MainTab:CreateSection("Color Pickerüòâ")

local ESPColor = Color3.fromRGB(255, 255, 255) -- Default

local ColorPicker = MainTab:CreateColorPicker({
    Name = "ESP Color",
    Color = ESPColor,
    Flag = "ColorPicker1",
    Callback = function(Value)
        ESPColor = Value
    end
})

local RainbowEnabled = false
local ESPColor = Color3.fromRGB(255, 255, 255)

local Toggle = MainTab:CreateToggle({
    Name = "Rainbow ESP",
    CurrentValue = false,
    Flag = "Toggle_RainbowESP",
    Callback = function(Value)
        RainbowEnabled = Value
    end,
})

-- Rainbow color updater (run this once, e.g., after toggle creation)
spawn(function()
    while true do
        if RainbowEnabled then
            -- Cycle through HSV hue based on time for smooth rainbow
            ESPColor = Color3.fromHSV(tick() % 5 / 5, 1, 1)  
        else
            -- Reset to white or any default color when disabled
            ESPColor = Color3.fromRGB(255, 255, 255)
        end
        wait(0.03) -- Update ~30 times per second
    end
end)


