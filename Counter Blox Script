local isUnlocked

local mt = getrawmetatable(game)
local oldNamecall = mt.__namecall
setreadonly(mt, false)

local isUnlocked

mt.__namecall = newcclosure(function(self, ...)
   local args = {...}
   if getnamecallmethod() == "InvokeServer" and tostring(self) == "Hugh" then
       return
   end
   if getnamecallmethod() == "FireServer" then
       if args[1] == LocalPlayer.UserId then
           return
       end
       if string.len(tostring(self)) == 38 then
           if not isUnlocked then
               isUnlocked = true
               for i,v in pairs(allSkins) do
                   local doSkip
                   for i2,v2 in pairs(args[1]) do
                       if v[1] == v2[1] then
                           doSkip = true
                       end
                   end
                   if not doSkip then
                       table.insert(args[1], v)
                   end
               end
           end
           return
       end
       if tostring(self) == "DataEvent" and args[1][4] then
           local currentSkin = string.split(args[1][4][1], "_")[2]
           if args[1][2] == "Both" then
               LocalPlayer["SkinFolder"]["CTFolder"][args[1][3]].Value = currentSkin
               LocalPlayer["SkinFolder"]["TFolder"][args[1][3]].Value = currentSkin
           else
               LocalPlayer["SkinFolder"][args[1][2] .. "Folder"][args[1][3]].Value = currentSkin
           end
       end
   end
   return oldNamecall(self, ...)
end)
   
setreadonly(mt, true)

Client.CurrentInventory = allSkins

local TClone, CTClone = LocalPlayer.SkinFolder.TFolder:Clone(), game.Players.LocalPlayer.SkinFolder.CTFolder:Clone()
LocalPlayer.SkinFolder.TFolder:Destroy()
LocalPlayer.SkinFolder.CTFolder:Destroy()
TClone.Parent = LocalPlayer.SkinFolder
CTClone.Parent = LocalPlayer.SkinFolder
   end,
})


local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Camera = workspace.CurrentCamera
local LocalPlayer = Players.LocalPlayer

local drawings = {}

local function createDrawingsForPlayer(player)
    if player == LocalPlayer then return end
    drawings[player] = {
        box = Drawing.new("Square"),
        tracer = Drawing.new("Line"),
        highlight = Instance.new("Highlight")
    }

    local box = drawings[player].box
    box.Thickness = 2
    box.Transparency = 1
    box.Filled = false

    local tracer = drawings[player].tracer
    tracer.Thickness = 2

    local chams = drawings[player].highlight
    chams.FillColor = ESPColor
    chams.OutlineColor = Color3.new(1, 1, 1)
    chams.FillTransparency = 0.5
    chams.OutlineTransparency = 0
    chams.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
end

local function removeDrawings(player)
    if drawings[player] then
        drawings[player].box:Remove()
        drawings[player].tracer:Remove()
        drawings[player].highlight:Destroy()
        drawings[player] = nil
    end
end

for _, player in ipairs(Players:GetPlayers()) do
    createDrawingsForPlayer(player)
end

Players.PlayerAdded:Connect(createDrawingsForPlayer)
Players.PlayerRemoving:Connect(removeDrawings)

RunService.RenderStepped:Connect(function()
    if not ESPEnabled then
        for _, data in pairs(drawings) do
            data.box.Visible = false
            data.tracer.Visible = false
            data.highlight.Enabled = false
        end
        return
    end

    for _, player in ipairs(Players:GetPlayers()) do
        if player == LocalPlayer then continue end
        local char = player.Character
        local data = drawings[player]
        if not char or not char:FindFirstChild("HumanoidRootPart") then
            if data then
                data.box.Visible = false
                data.tracer.Visible = false
                data.highlight.Enabled = false
            end
            continue
        end

        if TeamCheck and player.Team == LocalPlayer.Team then
            data.box.Visible = false
            data.tracer.Visible = false
            data.highlight.Enabled = false
            continue
        end

        local hrp = char.HumanoidRootPart
        local size = char:GetExtentsSize()
        local cf = hrp.CFrame

        local top = (cf * CFrame.new(0, size.Y/2, 0)).Position
        local bottom = (cf * CFrame.new(0, -size.Y/2, 0)).Position

        local top2D, onScreenTop = Camera:WorldToViewportPoint(top)
        local bottom2D, onScreenBottom = Camera:WorldToViewportPoint(bottom)

        if not (onScreenTop and onScreenBottom) then
            data.box.Visible = false
            data.tracer.Visible = false
            data.highlight.Enabled = false
            continue
        end

        local height = bottom2D.Y - top2D.Y
        local width = height * 0.6
        local pos = Vector2.new(top2D.X - width/2, top2D.Y)

        -- Box
        if BoxESP then
            data.box.Visible = true
            data.box.Color = ESPColor
            data.box.Size = Vector2.new(width, height)
            data.box.Position = pos
        else
            data.box.Visible = false
        end

        -- Tracer with origin selection
if TracerESP then
    data.tracer.Visible = true
    data.tracer.Color = ESPColor

    if TracerOrigin == "Top" then
        data.tracer.From = Vector2.new(Camera.ViewportSize.X / 2, 0)
    elseif TracerOrigin == "Cursor" then
        data.tracer.From = game:GetService("UserInputService"):GetMouseLocation()
    else -- Default to bottom
        data.tracer.From = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y)
    end

    data.tracer.To = Vector2.new(bottom2D.X, bottom2D.Y)
else
    data.tracer.Visible = false
end


        -- Chams
        if ChamsEnabled then
            data.highlight.Parent = char
            data.highlight.Enabled = true
            data.highlight.FillColor = ESPColor
        else
            data.highlight.Enabled = false
        end
    end
end)

